{
  "base": {
    "title": {
      "show": false,
      "text": "",
      "subtext": ""
    },
    "grid": {
      "top": 60,
      "bottom": 60,
      "left": 60,
      "right": 30
    },
    "legend": {
      "show": true,

      "top": "auto",
      "right": "auto",
      "bottom": 15,
      "left": "center",

      "textStyle": {
        "fontSize": 10
      }
    },
    "tooltip": {
      "show": true,
      "trigger": "axis"
    }
  },

  "line": {
    "__series": {
      "enable": true,
      "symbol": "emptyCircle",
      "symbolSize": 4,
      "smooth": true,
      "areaStyle": null
    },
    "xAxis": { "type": "category" },
    "yAxis": {}
  },

  "line-area": {
    "__series": {
      "areaStyle": {
        "opacity": 0.65
      }
    }
  },

  "bar": {
    "__series": {
      "enable": true
    },
    "xAxis": { "type": "category" },
    "yAxis": {}
  }
}
