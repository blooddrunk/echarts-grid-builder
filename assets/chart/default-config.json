{
  "base": {
    "title": {
      "show": false,
      "text": "",
      "subtext": ""
    },
    "grid": {
      "top": 60,
      "bottom": 60,
      "left": 60,
      "right": 30
    },
    "legend": {
      "show": true,

      "top": "auto",
      "right": "auto",
      "bottom": 15,
      "left": "center",

      "textStyle": {
        "fontSize": 10
      }
    },
    "tooltip": {
      "show": true,
      "trigger": "axis"
    }
  },

  "line": {
    "__series": {
      "enable": true,
      "symbolSize": 4,
      "smooth": true,
      "areaStyle": null
    },
    "xAxis": {
      "type": "category",
      "show": true,
      "axisLine": {
        "show": true
      },
      "axisTick": {
        "show": false
      },
      "splitLine": {
        "show": true
      },
      "axisLabel": {
        "show": true,
        "rotate": 0
      }
    },
    "yAxis": {
      "show": true,
      "axisLine": {
        "show": true
      },
      "axisTick": {
        "show": false
      },
      "splitLine": {
        "show": true
      }
    }
  },

  "line-area": {
    "__series": {
      "areaStyle": {
        "opacity": 0.65
      }
    }
  },

  "line-stack": {
    "__series": {
      "stack": "总量",

      "areaStyle": {
        "opacity": 0.65
      }
    }
  },

  "bar": {
    "__series": {
      "enable": true
    },
    "xAxis": { "show": true, "type": "category" },
    "yAxis": { "show": true }
  }
}
